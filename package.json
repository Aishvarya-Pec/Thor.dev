{
  "name": "thor-dev",
  "version": "1.0.0",
  "description": "Thor.dev - Multi-Agent Workspace for App Generation and Deployment",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:dashboard\" \"npm run ws:dev\"",
    "dev:dashboard": "npm run dev --workspace=apps/dashboard",
    "dev:landing": "npm run dev --workspace=apps/landing",
    "ws:dev": "npm run dev --workspace=packages/ws-server",
    "build": "npm run build --workspaces",
    "build:dashboard": "npm run build --workspace=apps/dashboard",
    "build:landing": "npm run build --workspace=apps/landing",
    "lint": "npm run lint --workspaces",
    "test": "npm run test --workspaces",
    "db:generate": "npm run db:generate --workspace=apps/dashboard",
    "db:push": "npm run db:push --workspace=apps/dashboard",
    "db:migrate": "npm run db:migrate --workspace=apps/dashboard",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules apps/*/.next",
    "postinstall": "npm run db:generate"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "thor.dev",
    "multi-agent",
    "workspace",
    "nextjs",
    "ai",
    "code-generation",
    "deployment"
  ],
  "author": "Thor.dev Team",
  "license": "MIT"
}